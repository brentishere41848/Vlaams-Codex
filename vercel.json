{
  "version": 2,
  "builds": [{ "src": "api/chat.js", "use": "@vercel/node" }],
  "functions": {
    "api/chat.js": { "maxDuration": 60 },
    "website/api/chat.js": { "maxDuration": 60 }
  },
  "routes": [
    { "handle": "filesystem" },
    { "src": "^/$", "dest": "/website/index.html" },
    { "src": "^/ai/?$", "dest": "/website/ai/index.html" },
    { "src": "^/ai/(.*)$", "dest": "/website/ai/$1" },
    { "src": "^/docs/?$", "dest": "/website/docs/index.html" },
    { "src": "^/docs/(.*)$", "dest": "/website/docs/$1" },
    { "src": "^/playground/?$", "dest": "/website/playground/index.html" },
    { "src": "^/playground/(.*)$", "dest": "/website/playground/$1" },
    { "src": "/(.*)", "dest": "/website/$1" }
  ]
}
